<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="CopyPaths" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorNextWordWithSelection" />
  <action id="GotoCustomRegion" />
  <action id="GotoImplementation">
    <mouse-shortcut keystroke="control alt button1" />
  </action>
  <action id="IncreaseColumnWidth" />
  <action id="IncrementWindowWidth" />
  <action id="IntroduceField" />
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="shift ctrl right" />
  </action>
  <action id="ResizeToolWindowRight" />
</keymap>