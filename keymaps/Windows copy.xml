  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="CopyPaths" />
  <action id="ShowExecutionPoint" />
  <action id="com.jetbrains.plugins.remotesdk.console.RunSshConsoleAction">
    <keyboard-shortcut first-keystroke="alt f10" />
  </action>
</keymap>